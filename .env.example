# Database Configuration
DATABASE_NAME=gymgem_db
DATABASE_USER=your_database_user
DATABASE_PASSWORD=your_database_password
DATABASE_HOST=localhost
DATABASE_PORT=5432

# Redis Configuration (for Django Channels)
# Local Redis (default)
REDIS_URL=redis://127.0.0.1:6379/0

# For Redis with password
# REDIS_URL=redis://:yourpassword@127.0.0.1:6379/0

# For Redis Cloud or managed Redis services
# REDIS_URL=redis://username:password@hostname:port/database

# Django Secret Key
SECRET_KEY=your-secret-key-here

# Debug Mode (set to False in production)
DEBUG=True

# Allowed Hosts (comma-separated)
ALLOWED_HOSTS=localhost,127.0.0.1
